{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "noImplicitAny": false,
    "rootDir": "./src",
    "baseUrl": ".",
    "paths": {
      "~*": ["./src/*"]
    },
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "lib": ["dom", "es2015"]
  }
}
